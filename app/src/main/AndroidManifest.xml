<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sigit.tempat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="sigit.tempat.MainActivity" />
        <activity android:name="sigit.tempat.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sigit.tempat.LoginActivity" />
        <activity android:name="sigit.tempat.TempatActivity" android:parentActivityName="sigit.tempat.MainActivity">

        </activity>
    </application>

</manifest>